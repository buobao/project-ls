        <RelativeLayout
            android:id="@+id/rlyt_password_main_page_slid_menus"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:paddingLeft="33dp" >

            <ImageView
                android:id="@+id/img_password_main_page_slid_menus"
                android:layout_width="16dp"
                android:layout_height="16dp"
                android:layout_centerVertical="true"
                android:src="@drawable/homeside_password_icon" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@+id/img_password_main_page_slid_menus"
                android:gravity="center_vertical|left"
                android:text="输PIN码"
                 android:textColor="#F4F5F6"
                android:textSize="18dp"  />
        </RelativeLayout>

        <View
            android:id="@+id/view_line_6"
            android:layout_below="@+id/rlyt_password_main_page_slid_menus"
            android:layout_width="match_parent"
            android:layout_height="0.33dp"
             android:layout_marginLeft="33dp"
            android:background="#D1D1D1" />

        <RelativeLayout
            android:id="@+id/rlyt_sacn_customer_main_page_slid_menus"
            android:layout_below="@+id/view_line_6"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:paddingLeft="33dp" >

            <ImageView
                android:id="@+id/img_scan_main_page_slid_menus"
                android:layout_width="16dp"
                android:layout_height="16dp"
                android:layout_centerVertical="true"
                android:src="@drawable/homeside_scan_icon" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@+id/img_scan_main_page_slid_menus"
                android:gravity="center_vertical|left"
                android:text="扫一扫"
                 android:textColor="#F4F5F6"
                android:textSize="18dp"  />
        </RelativeLayout>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rylt_buttom_sild_menu"
        android:layout_below="@+id/rylt_mid_3_sild_menu"
        android:layout_width="250dp"
        android:layout_height="45dp"
        android:layout_marginTop="8dp"
        >
        <RelativeLayout
            android:id="@+id/rlyt_remind_customer_main_page_slid_menus"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:paddingLeft="33dp" >

            <ImageView
                android:id="@+id/img_remind_main_page_slid_menus"
