                android:id="@+id/img_my_house_main_page_slid_menus2"
                android:layout_width="16dp"
                android:layout_height="16dp"
                android:layout_centerVertical="true"
                android:src="@drawable/homeside_myhouse_icon" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@+id/img_my_house_main_page_slid_menus2"
                android:gravity="center_vertical|left"
                android:text="我的出租"
                android:textColor="#F4F5F6"
                android:textSize="18dp"  />
        </RelativeLayout>
        <View
            android:id="@+id/view_line_3"
            android:layout_below="@+id/rlyt_my_house_main_page_slid_menus2"
            android:layout_width="match_parent"
            android:layout_height="0.33dp"
              android:layout_marginLeft="33dp"
            android:background="#D1D1D1" />

        <RelativeLayout
            android:id="@+id/rlyt_key_house_main_page_slid_menus"
            android:layout_below="@+id/view_line_3"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:paddingLeft="33dp" >

            <ImageView
                android:id="@+id/img_key_house_main_page_slid_menus"
                android:layout_width="16dp"
                android:layout_height="16dp"
                android:layout_centerVertical="true"
                android:src="@drawable/homeside_key_icon" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@+id/img_key_house_main_page_slid_menus"
                android:gravity="center_vertical|left"
                android:text="钥匙管理"
                 android:textColor="#F4F5F6"
                android:textSize="18dp"  />
        </RelativeLayout>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rylt_mid_2_sild_menu"
        android:layout_width="250dp"
        android:layout_height="182dp"
        android:layout_marginTop="8dp"
        android:layout_below="@+id/rylt_mid_1_sild_menu"
