<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:left="-5dp"
        android:right="-5dp"
        android:top="-5dp">
        <shape >
            <solid
                android:color="@android:color/white"/>
            <stroke
                android:width="2px"
                android:color="@color/gray_text_color"
                android:dashGap="4dp"
                android:dashWidth="7dp">
            </stroke>
        </shape>
    </item>
</layer-list>
