<?xml version="1.0" encoding="utf-8"?>

<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="250dp"
    android:layout_height="match_parent"
    android:background="@color/transparent_half"
    android:paddingTop="48dp"
    android:orientation="vertical"
    >
    <ScrollView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scrollbars="none"
        >
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        >
    <LinearLayout
        android:id="@+id/rlyt_top_sild_menu"
        android:layout_width="250dp"
        android:layout_height="137dp"
        android:orientation="vertical"
        >

        <RelativeLayout
            android:id="@+id/rlyt_sell_house_main_page_slid_menus"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:paddingLeft="30dp" >

            <ImageView
                android:id="@+id/img_sell_house_main_page_slid_menus"
                android:layout_width="16dp"
                android:layout_height="16dp"
                android:layout_centerVertical="true"
                android:src="@drawable/homeside_sell_icon" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@+id/img_sell_house_main_page_slid_menus"
                android:gravity="center_vertical|left"
                android:text="@string/house_chushou"
                android:textColor="#F4F5F6"
                android:textSize="18dp" />
        </RelativeLayout>

        <View
            android:id="@+id/view_line_1"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_marginLeft="33dp"
            android:background="#D1D1D1" />

        <RelativeLayout
            android:id="@+id/rlyt_rent_house_main_page_slid_menus"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:paddingLeft="33dp" >

            <ImageView
                android:id="@+id/img_rent_house_main_page_slid_menus"
                android:layout_width="16dp"
                android:layout_height="16dp"
                android:layout_centerVertical="true"
                android:src="@drawable/homeside_rent_icon" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@+id/img_rent_house_main_page_slid_menus"
                android:gravity="center_vertical|left"
                android:text="@string/house_chuzu"
                 android:textColor="#F4F5F6"
                android:textSize="18dp"  />
        </RelativeLayout>

        <View
            android:id="@+id/view_line_2"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_marginLeft="33dp"
            android:background="#D1D1D1"/>

        <RelativeLayout
            android:id="@+id/rlyt_see_house_main_page_slid_menus"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:paddingLeft="33dp" >

            <ImageView
                android:id="@+id/img_see_house_main_page_slid_menus"
                android:layout_width="16dp"
                android:layout_height="16dp"
                android:layout_centerVertical="true"
                android:src="@drawable/homeside_see_icon" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@+id/img_see_house_main_page_slid_menus"
                android:gravity="center_vertical|left"
                android:text="约看房源"
                android:textColor="#F4F5F6"
                android:textSize="18dp"  />
        </RelativeLayout>
    </LinearLayout>

    <RelativeLayout
        android:id="@+id/rylt_mid_1_sild_menu"
        android:layout_width="250dp"
        android:layout_height="136dp"
        android:layout_below="@+id/rlyt_top_sild_menu"
        android:layout_marginTop="8dp"
        >

        <RelativeLayout
            android:id="@+id/rlyt_my_house_main_page_slid_menus"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:paddingLeft="30dp" >

            <ImageView
                android:id="@+id/img_my_house_main_page_slid_menus"
                android:layout_width="16dp"
                android:layout_height="16dp"
                android:layout_centerVertical="true"
                android:src="@drawable/homeside_myhouse_icon" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@+id/img_my_house_main_page_slid_menus"
