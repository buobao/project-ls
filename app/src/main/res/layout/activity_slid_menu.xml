                android:gravity="center_vertical|left"
                android:text="抢公租"
                android:textColor="#F4F5F6"
                android:textSize="18dp"  />
        </RelativeLayout>

        <View
            android:id="@+id/view_line_52"
            android:layout_below="@+id/rlyt_grab_house_main_page_slid_menus2"
            android:layout_width="match_parent"
            android:layout_height="0.33dp"
            android:layout_marginLeft="33dp"
            android:background="#D1D1D1"/>
        <RelativeLayout
            android:id="@+id/rlyt_grab_customer_main_page_slid_menus"
            android:layout_below="@+id/view_line_52"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:paddingLeft="33dp" >

            <ImageView
                android:id="@+id/img_grab_customer_main_page_slid_menus"
                android:layout_width="16dp"
                android:layout_height="16dp"
                android:layout_centerVertical="true"
                android:src="@drawable/homeside_grabcustomer_icon" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="10dp"
